{
  "name": "jupiter",
  "version": "1.0.0",
  "main": "server.ts",
  "repository": "https://github.com/wendelpaco/jupiter.git",
  "author": "wendel paco",
  "license": "MIT",
  "scripts": {
    "dev": "tsnd --inspect --transpile-only --respawn --ignore-watch node_modules -r tsconfig-paths/register src/server.ts",
    "build": "tsc --project tsconfig.json && tscpaths -p tsconfig.json -s ./src -o ./dist",
    "start": "node ./dist/server.js"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "mongoose": "^5.10.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.7",
    "@types/express": "^4.17.7",
    "@types/mongoose": "^5.7.36",
    "@types/morgan": "^1.9.1",
    "rimraf": "^3.0.2",
    "ts-node-dev": "^1.0.0-pre.61",
    "tsconfig-paths": "^3.9.0",
    "tscpaths": "^0.0.9",
    "typescript": "^4.0.2"
  }
}